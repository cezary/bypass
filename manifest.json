{
  "name": "Bypass",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Allows you to read paywalled articles",
  "icons": {
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://*.baltimoresun.com/",
    "*://*.chicagobusiness.com/",
    "*://*.chicagotribune.com/",
    "*://*.dailypress.com/",
    "*://*.economist.com/",
    "*://*.ft.com/",
    "*://*.courant.com/",
    "*://*.hbr.org/",
    "*://*.jacksonville.com/",
    "*://*.latimes.com/",
    "*://*.law360.com/",
    "*://*.mcall.com/",
    "*://*.medscape.com/",
    "*://*.newyorker.com/",
    "*://*.orlandosentinel.com/",
    "*://*.quora.com/",
    "*://*.telegraph.co.uk/",
    "*://*.theage.com.au/",
    "*://*.tribdss.com/",
    "*://*.sun-sentinel.com/",
    "*://*.wsj.com/",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ]
}
